pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                echo "Code cloned"
            }
        }

        stage('Build') {
            steps {
                echo "Build success"
            }
        }

        stage('Test') {
            steps {
                echo "Testing done"
            }
        }

        stage('Deploy') {
            steps {
                echo "Deploy done"
            }
        }
    }
}

